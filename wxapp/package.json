{
  "name": "nice-router-taro",
  "version": "1.0.0",
  "private": true,
  "description": "Nice-Router of Taro",
  "templateInfo": {
    "name": "redux",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "format": "yarn prettier && yarn lint --fix",
    "prettier": "prettier --write \"./src/**/*\" ",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src && npm run lint:style",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src && npm run lint:style",
    "lint:style": "stylelint 'src/**/*.scss' --syntax scss",
    "precommit": "lint-staged"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "Kala <kalaliu@aliyun.com>",
  "license": "MIT",
  "dependencies": {
    "@tarojs/cli": "3.1.1",
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "3.1.1",
    "@tarojs/react": "3.1.1",
    "@tarojs/runtime": "3.1.1",
    "@tarojs/taro": "3.1.1",
    "@tarojs/taro-h5": "3.1.1",
    "async-validator": "^3.2.4",
    "classnames": "^2.2.6",
    "crypto-es": "^1.2.7",
    "dva-core": "^2.0.2",
    "lodash": "^4.17.11",
    "nervjs": "^1.5.7",
    "path-to-regexp": "^3.0.0",
    "qr.js": "^0.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "taro-ui": "^3.0.0-alpha"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/mini-runner": "3.1.1",
    "@tarojs/webpack-runner": "3.1.1",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-taro": "3.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "stylelint": "^13.3.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.16.0",
    "eslint-plugin-taro": "3.1.1",
    "eslint-config-taro": "3.1.1",
    "lint-staged": "^7.2.2",
    "lodash-webpack-plugin": "^0.11.5",
    "miniprogram-api-typings": "^3.2.2",
    "prettier": "^2.2.1",
    "stylelint-config-prettier": "^8.0.2",
    "typescript": "4.1.5",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,scss}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{ts,tsx,js,jsx}": "npm run lint-staged:js",
    "**/*.scss": "stylelint --syntax scss"
  }
}
